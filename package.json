{
  "name": "backbone.deepmodel",
  "version": "0.1.2",
  "description": "Backbone.DeepModel plugin",
  "main": "./dist/backbone.deepmodel",
  "scripts": {
    "test": "karma start --single-run",
    "test-watch": "karma start --auto-watch",
    "coveralls": "cat coverage/report-lcov/lcov.info | coveralls",
    "eslint": "eslint --ignore-path .gitignore --ignore-pattern dist,coverage .",
    "jscs": "jscs .",
    "lint": "npm run eslint && npm run jscs",
    "build": "webpack",
    "fullbuild": "npm run lint && npm test && npm run build",
    "examples": "http-server . -o -c-1",
    "benchmark": "ls benchmark/*.js | xargs -I {} node {}",
    "check-version": "./scripts/check-version",
    "version": "npm run check-version && npm run fullbuild && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "lib"
  ],
  "keywords": [
    "backbone"
  ],
  "author": "ybiquitous",
  "license": "MIT",
  "repository": "ybiquitous/backbone.deepmodel",
  "homepage": "https://github.com/ybiquitous/backbone.deepmodel",
  "dependencies": {
    "backbone": ">=1.2.0"
  },
  "devDependencies": {
    "babel-core": "6.4.0",
    "babel-loader": "6.2.1",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-transform-object-assign": "6.3.13",
    "babel-preset-es2015": "6.3.13",
    "benchmark": "2.0.0",
    "chai": "3.4.1",
    "coveralls": "2.11.6",
    "eslint": "1.10.3",
    "http-server": "0.8.5",
    "istanbul-instrumenter-loader": "0.1.3",
    "jquery": "latest",
    "jscs": "2.8.0",
    "karma": "0.13.19",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.2",
    "karma-coverage": "0.5.3",
    "karma-firefox-launcher": "0.1.7",
    "karma-ie-launcher": "0.2.0",
    "karma-mocha": "0.2.1",
    "karma-sinon": "1.0.4",
    "karma-webpack": "1.7.0",
    "mocha": "2.3.4",
    "sinon": "1.17.2",
    "webpack": "1.12.11"
  },
  "engines": {
    "node": ">=0.12"
  }
}
