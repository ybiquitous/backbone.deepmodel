{
  "name": "backbone.deepmodel",
  "version": "0.3.3",
  "description": "Backbone.DeepModel plugin",
  "main": "./dist/backbone.deepmodel",
  "scripts": {
    "test": "karma start --single-run",
    "test:watch": "karma start --auto-watch",
    "test:backbone1.2": "./scripts/test-backbone1.2",
    "lint": "eslint --max-warnings=0 .",
    "build": "webpack",
    "fullbuild": "npm run lint && npm test && npm run build",
    "examples": "http-server . -o -c-1 -i false",
    "benchmark": "ls benchmark/*.js | xargs -I {} node {}",
    "check-version": "node ./scripts/check-version.js",
    "version": "npm run check-version && npm run fullbuild && git add -A dist",
    "postversion": "git push && git push --tags && npm run github-release",
    "commit": "commit-wizard",
    "github-release": "node -r dotenv/config node_modules/conventional-github-releaser/cli.js -p angular",
    "doc": "esdoc -c esdoc.json"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "lib"
  ],
  "keywords": [
    "backbone"
  ],
  "author": "ybiquitous <10koba01@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ybiquitous/backbone.deepmodel.git"
  },
  "homepage": "https://github.com/ybiquitous/backbone.deepmodel",
  "dependencies": {
    "backbone": ">=1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.13.0",
    "benchmark": "^2.1.2",
    "chai": "^3.5.0",
    "conventional-github-releaser": "^1.1.2",
    "dotenv": "^2.0.0",
    "esdoc": "^0.4.6",
    "eslint": "^3.10.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "http-server": "^0.9.0",
    "jquery": "^3.0.0",
    "karma": "^1.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jsdom-launcher": "^4.0.0",
    "karma-mocha": "^1.1.0",
    "karma-sauce-launcher": "^1.0.0",
    "karma-threshold-reporter": "^0.1.15",
    "karma-webpack": "^1.7.0",
    "mocha": "^3.0.0",
    "pre-git": "^3.8.3",
    "sinon": "^2.0.0-pre.2",
    "webpack": "^1.13.3"
  },
  "engines": {
    "node": ">=4"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional",
      "pre-commit": [
        "npm run lint",
        "npm test"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-merge": []
    }
  }
}
