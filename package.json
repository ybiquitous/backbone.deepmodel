{
  "name": "backbone.deepmodel",
  "version": "1.0.1",
  "description": "Backbone.DeepModel plugin",
  "main": "lib/index.js",
  "scripts": {
    "test": "karma start --single-run",
    "test:watch": "karma start --auto-watch",
    "test:backbone1.2": "./scripts/test-backbone1.2",
    "lint": "eslint --max-warnings=0 .",
    "build": "webpack",
    "fullbuild": "npm run lint && npm test && npm run build",
    "examples": "http-server . -o -c-1 -i false",
    "benchmark": "ls benchmark/*.js | xargs -I {} node {}",
    "check-version": "node ./scripts/check-version.js",
    "version": "npm run check-version && npm run fullbuild && git add -A dist",
    "postversion": "git push && git push --tags",
    "github-release": "conventional-github-releaser -p angular",
    "doc": "esdoc -c esdoc.json",
    "precommit": "lint-staged",
    "commitmsg": "validate-commit-msg",
    "commit": "commit-wizard"
  },
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "backbone"
  ],
  "author": "ybiquitous <ybiquitous@gmail.com>",
  "license": "MIT",
  "repository": "ybiquitous/backbone.deepmodel",
  "homepage": "https://github.com/ybiquitous/backbone.deepmodel",
  "dependencies": {
    "backbone": ">=1.2.0"
  },
  "devDependencies": {
    "babel-core": "*",
    "babel-loader": "*",
    "babel-plugin-add-module-exports": "*",
    "babel-plugin-istanbul": "*",
    "babel-plugin-transform-object-assign": "*",
    "babel-preset-env": "*",
    "babel-preset-power-assert": "*",
    "benchmark": "*",
    "chai": "*",
    "conventional-github-releaser": "*",
    "dotenv": "*",
    "esdoc": "0.4",
    "eslint": "3",
    "eslint-config-ybiquitous": "*",
    "http-server": "*",
    "husky": "*",
    "jquery": "*",
    "jsdom": "*",
    "karma": "*",
    "karma-coverage": "*",
    "karma-jsdom-launcher": "*",
    "karma-mocha": "*",
    "karma-sauce-launcher": "*",
    "karma-sourcemap-loader": "*",
    "karma-threshold-reporter": "*",
    "karma-webpack": "*",
    "lint-staged": "*",
    "mocha": "*",
    "power-assert": "*",
    "sinon": "*",
    "validate-commit-msg": "*",
    "webpack": "*"
  },
  "engines": {
    "node": ">=6"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --max-warnings=0",
      "git add"
    ]
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    },
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [],
      "pre-push": [],
      "post-commit": [],
      "post-checkout": [],
      "post-merge": []
    }
  }
}
