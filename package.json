{
  "name": "backbone.deepmodel",
  "version": "1.1.0",
  "description": "Backbone.DeepModel plugin",
  "main": "lib/index.js",
  "scripts": {
    "test": "karma start --single-run",
    "test:watch": "karma start --auto-watch",
    "test:backbone1.2": "./scripts/test-backbone1.2",
    "lint": "eslint --max-warnings=0 .",
    "build": "webpack",
    "fullbuild": "npm run lint && npm test && npm run build",
    "examples": "http-server . -o -c-1 -i false",
    "benchmark": "ls benchmark/*.js | xargs -I {} node {}",
    "prerelease": "npm run lint && npm test",
    "release": "standard-version --no-verify --commit-all",
    "release:github": "conventional-github-releaser -p angular",
    "doc": "esdoc -c esdoc.json",
    "precommit": "lint-staged",
    "commitmsg": "validate-commit-msg"
  },
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "backbone",
    "plugin"
  ],
  "author": "ybiquitous <ybiquitous@gmail.com>",
  "license": "MIT",
  "repository": "ybiquitous/backbone.deepmodel",
  "homepage": "https://github.com/ybiquitous/backbone.deepmodel",
  "peerDependencies": {
    "backbone": ">=1.2.0"
  },
  "devDependencies": {
    "babel-core": "*",
    "babel-loader": "*",
    "babel-plugin-add-module-exports": "*",
    "babel-plugin-istanbul": "*",
    "babel-plugin-transform-object-assign": "*",
    "babel-preset-env": "*",
    "babel-preset-power-assert": "*",
    "backbone": ">=1.2.0",
    "benchmark": "*",
    "conventional-github-releaser": "*",
    "dotenv": "*",
    "esdoc": "*",
    "esdoc-standard-plugin": "*",
    "eslint": "*",
    "eslint-config-ybiquitous": "*",
    "http-server": "*",
    "husky": "*",
    "jquery": "*",
    "jsdom": "*",
    "karma": "*",
    "karma-coverage": "*",
    "karma-jsdom-launcher": "*",
    "karma-mocha": "*",
    "karma-sauce-launcher": "*",
    "karma-sourcemap-loader": "*",
    "karma-threshold-reporter": "*",
    "karma-webpack": "*",
    "lint-staged": "*",
    "mocha": "*",
    "power-assert": "*",
    "sinon": "*",
    "standard-version": "*",
    "validate-commit-msg": "*",
    "webpack": "*"
  },
  "engines": {
    "node": ">=6"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --max-warnings=0 --no-ignore",
      "git add"
    ]
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "standard-version": {
    "scripts": {
      "postbump": "node ./scripts/update-version.js && npm run build && git add -A dist",
      "posttag": "node ./scripts/update-version.js --revert && git commit -m \"chore: revert to dummy version\""
    }
  }
}
