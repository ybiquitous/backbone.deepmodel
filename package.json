{
  "name": "backbone.deepmodel",
  "version": "0.3.1",
  "description": "Backbone.DeepModel plugin",
  "main": "./dist/backbone.deepmodel",
  "scripts": {
    "test": "karma start --single-run",
    "test:quick": "karma start --single-run --browsers PhantomJS",
    "test:watch": "karma start --auto-watch --browsers PhantomJS",
    "test:backbone1.2": "npm install backbone@1.2 && npm run test && npm install backbone",
    "coveralls": "cat coverage/report-lcov/lcov.info | coveralls",
    "lint": "standard",
    "build": "webpack",
    "fullbuild": "npm run lint && npm test && npm run build",
    "examples": "http-server . -o -c-1 -i false",
    "benchmark": "ls benchmark/*.js | xargs -I {} node {}",
    "check-version": "node ./scripts/check-version",
    "version": "npm run check-version && npm run fullbuild && git add -A dist",
    "postversion": "git push && git push --tags && npm run github-release",
    "commit": "commit-wizard",
    "upgrade": "npm-check-updates --upgrade --dev",
    "github-release": "conventional-github-releaser -p angular",
    "doc": "esdoc -c esdoc.json"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "lib"
  ],
  "keywords": [
    "backbone"
  ],
  "author": "ybiquitous <10koba01@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ybiquitous/backbone.deepmodel.git"
  },
  "homepage": "https://github.com/ybiquitous/backbone.deepmodel",
  "dependencies": {
    "backbone": ">=1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "benchmark": "^2.1.0",
    "chai": "^3.5.0",
    "conventional-github-releaser": "^1.1.2",
    "coveralls": "^2.11.9",
    "esdoc": "^0.4.6",
    "http-server": "^0.9.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jquery": "^3.0.0",
    "karma": "^1.0.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-threshold-reporter": "^0.1.15",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "npm-check-updates": "^2.7.2",
    "phantomjs-prebuilt": "^2.1.7",
    "pre-git": "^3.8.3",
    "sinon": "^2.0.0-pre.2",
    "standard": "^7.0.0",
    "webpack": "^1.13.0"
  },
  "engines": {
    "node": ">=0.12"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional",
      "pre-commit": [
        "npm run lint",
        "npm run test:quick"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-merge": []
    }
  },
  "standard": {
    "ignore": [
      "dist/**"
    ]
  }
}
