{
  "name": "backbone.deepmodel",
  "version": "0.2.2",
  "description": "Backbone.DeepModel plugin",
  "main": "./dist/backbone.deepmodel",
  "scripts": {
    "test": "karma start --single-run --browsers PhantomJS",
    "test:all": "karma start --single-run",
    "test:watch": "karma start --auto-watch --browsers PhantomJS",
    "test:backbone1.2": "npm install backbone@1.2 && npm run test:all && npm install backbone",
    "coveralls": "cat coverage/report-lcov/lcov.info | coveralls",
    "eslint": "eslint .",
    "jscs": "jscs .",
    "lint": "npm run eslint && npm run jscs",
    "build": "webpack",
    "fullbuild": "npm run lint && npm test && npm run build",
    "examples": "http-server . -o -c-1 -i false",
    "benchmark": "ls benchmark/*.js | xargs -I {} node {}",
    "check-version": "./scripts/check-version",
    "version": "npm run check-version && npm run fullbuild && git add -A dist",
    "postversion": "git push && git push --tags",
    "commit": "git-cz"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "lib"
  ],
  "keywords": [
    "backbone"
  ],
  "author": "ybiquitous <10koba01@gmail.com>",
  "license": "MIT",
  "repository": "ybiquitous/backbone.deepmodel",
  "homepage": "https://github.com/ybiquitous/backbone.deepmodel",
  "dependencies": {
    "backbone": ">=1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "benchmark": "^2.1.0",
    "chai": "^3.5.0",
    "commitizen": "^2.6.1",
    "coveralls": "^2.11.8",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^2.2.0",
    "http-server": "^0.9.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jquery": "^2.2.1",
    "jscs": "^2.10.1",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.5",
    "karma-firefox-launcher": "^0.1.7",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.4",
    "karma-threshold-reporter": "^0.1.15",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.4",
    "pre-git": "^3.4.0",
    "sinon": "^1.17.3",
    "webpack": "^1.12.14"
  },
  "engines": {
    "node": ">=0.12"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "pre-git": {
      "commit-msg": "conventional",
      "pre-commit": [
        "npm run lint",
        "npm test"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-merge": []
    }
  }
}
